/dts-v1/;

/include/ "apm-storm.dtsi"

/ {
	model = "APM X-Gene Mustang board";
	compatible = "apm,mustang", "apm,xgene-storm";

	memreserve = <0x40 0x00000000 0x00 0x00010000>; /* SMP trampoline */

	memory {
		#address-cells = <2>;
		#size-cells = <2>;
		device_type = "memory";
		reg = <0x00 0x00000000 0x00 0x02000000>;
	};

	chosen {
		stdout-path = "/soc/serial@1c020000";
	};
};

&pcie0clk {
	status = "ok";
};

&pcie0 {
	status = "ok";
};

&serial0 {
	status = "ok";
};

&menet {
	status = "ok";
};

&sgenet0 {
	status = "ok";
};

&xgenet {
	status = "ok";
};

&sata2 {
	status = "ok";
};

&sata3 {
	status = "ok";
};

&usb0 {
	status = "ok";
};

&usb1 {
	status = "ok";
};
